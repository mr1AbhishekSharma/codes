#include <stdio.h>
int main(){
    int A[]={4,6,2,9,7,8,3};
    int n=sizeof(A)/sizeof(A[0]);
    int data=7;
    int found=0;
    for(int i=0;i<n;i++){
        if(A[i]==data){
            printf("element found at index %d",i);
            found=1;
            break;
        }
    }
    if(found==0){
        printf("Element not found");
    }
    return 0;
}
